{
  "name": "instagram-profilecrawl",
  "version": "2.0.1",
  "description": "Quickly crawl the information (e.g. followers, tags, etc...) of an instagram profile. No login required!",
  "license": "MIT",
  "author": {
    "name": "Nacim Goura",
    "email": "nacim.goura.dev@gmail.com",
    "url": "https://nacimgoura.github.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nacimgoura/instagram-profilecrawl.git"
  },
  "engines": {
    "node": ">=7.6"
  },
  "scripts": {
    "start": "node index.js",
    "test": "ava",
    "lint": "xo"
  },
  "main": "index.js",
  "bin": {
    "instagram-profilecrawl": "index.js"
  },
  "preferGlobal": true,
  "keywords": [
    "automation",
    "instagram",
    "selenium",
    "crawl",
    "bot",
    "javascript",
    "puppeteer",
    "headless",
    "chromedriver"
  ],
  "dependencies": {
    "meow": "^5.0.0",
    "ora": "^2.1.0",
    "puppeteer": "^1.3.0",
    "write-yaml": "^1.0.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "execa": "^0.10.0",
    "xo": "^0.20.3"
  },
  "xo": {
    "prettier": true,
    "env": [
      "node",
      "browser"
    ],
    "rules": {
      "no-await-in-loop": "off",
      "no-implicit-coercion": "off",
      "no-extra-boolean-cast": "off"
    }
  },
  "prettier": {
    "bracketSpacing": true,
    "trailingComma": "none",
    "singleQuote": true
  }
}
